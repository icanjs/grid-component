  "homepage": "https://github.com/icanjs/grid-component#readme",
  "dependencies": {
    "can-component": "^3.0.7",
    "can-compute": "^3.0.8",
    "can-construct": "^3.1.1",
    "can-define": "^1.0.17",
    "can-event": "^3.3.0",
    "can-stache": "^3.0.22",
    "can-stache-bindings": "^3.0.14",
    "can-util": "^3.3.6",
    "can-view-autorender": "^3.0.4",
    "can-view-model": "^3.1.3",
