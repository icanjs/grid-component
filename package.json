    "can-construct": "^3.3.1",
    "can-define": "^2.0.3",
    "can-queues": "^1.0.1",
    "can-stache": "^4.2.3",
    "can-stache-bindings": "^4.1.0",
    "can-util": "^3.11.5",
    "can-view-autorender": "^4.0.0",
